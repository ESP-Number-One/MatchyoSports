    "tslib": "^2.6.2",
    "tsoa": "^5.1.1",
    "tsx": "^3.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@yarnpkg/sdks": "^3.0.0",
